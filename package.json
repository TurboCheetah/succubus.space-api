{
  "name": "succubus.space",
  "version": "2.5.0",
  "description": "a fast API for fetching hentai data built with Redis and MongoDB",
  "author": "Turbo",
  "license": "ISC",
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "start:docker": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint --fix && prettier --write src/",
    "docker": "docker-compose -f \"docker-compose.yml\" up -d --build",
    "docker:dev": "docker-compose -f \"docker-compose.dev.yml\" up -d --build",
    "test": "yarn docker:dev && jest",
    "patch": "standard-version --release-as patch",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/succubus.space-api.git"
  },
  "bugs": {
    "url": "https://github.com/TurboCheetah/succubus.space-api/issues"
  },
  "homepage": "https://github.com/TurboCheetah/succubus.space-api#readme",
  "dependencies": {
    "@aero/centra": "1.0.8",
    "@sentry/node": "6.15.0",
    "apollo-server-cache-redis": "3.3.0",
    "apollo-server-express": "3.5.0",
    "bull": "4.1.1",
    "cheerio": "1.0.0-rc.10",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv-cra": "3.0.2",
    "express": "4.17.1",
    "graphql": "15.7.2",
    "helmet": "4.6.0",
    "hpp": "0.2.3",
    "ioredis": "4.28.0",
    "jest": "27.3.1",
    "mongoose": "6.0.13",
    "morgan": "1.10.0",
    "nhentai": "3.0.0",
    "node-cron": "3.0.0",
    "rate-limiter-flexible": "2.3.4",
    "redis-json": "6.0.2",
    "reflect-metadata": "0.1.13",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "type-graphql": "1.1.1",
    "typescript": "4.5.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/bull": "3.15.5",
    "@types/cheerio": "0.22.30",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/eslint": "^7",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/helmet": "4.0.0",
    "@types/hpp": "0.2.2",
    "@types/ioredis": "4.28.1",
    "@types/jest": "27.0.3",
    "@types/mongoose": "5.11.97",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.9",
    "@types/node-cron": "3.0.0",
    "@types/supertest": "2.0.11",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "node-config": "0.0.2",
    "node-gyp": "8.4.0",
    "nodemon": "2.0.15",
    "prettier": "2.4.1",
    "standard-version": "9.3.2",
    "supertest": "6.1.6",
    "tsc-alias": "1.4.1",
    "tsconfig-paths": "3.12.0"
  },
  "lint-staged": {
    "src/*.ts": [
      "yarn lint:fix",
      "yarn test --bail --watchAll=false --findRelatedTests"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
