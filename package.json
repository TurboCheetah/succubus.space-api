{
  "name": "succubus.space",
  "version": "2.7.0",
  "description": "a fast API for fetching hentai data built with Redis and MongoDB",
  "author": "Turbo",
  "license": "MIT",
  "scripts": {
    "start": "wireit",
    "dev": "wireit",
    "build": "wireit",
    "lint": "wireit",
    "lint:fix": "wireit",
    "test": "yarn docker:dev && jest",
    "patch": "standard-version --release-as patch",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "wireit": {
    "start": {
      "command": "cross-env NODE_ENV=production node dist/server.js",
      "dependencies": [
        "build"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "dev": {
      "command": "cross-env NODE_ENV=development nodemon",
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "build": {
      "command": "tsc --pretty -p tsconfig.prod.json && tsc-alias",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.prod.json",
        ".tsbuildinfo"
      ],
      "output": [
        "dist/**"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "lint": {
      "command": "eslint --ignore-path .gitignore --ext .ts src/",
      "files": [
        "src/**/*.ts",
        ".eslintrc.json"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "lint:fix": {
      "command": "eslint --ignore-path .gitignore --ext .ts src/ --fix && prettier --write src/",
      "files": [
        "src/**/*.ts",
        ".eslintrc.json",
        ".prettierrc"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/succubus.space-api.git"
  },
  "bugs": {
    "url": "https://github.com/TurboCheetah/succubus.space-api/issues"
  },
  "homepage": "https://github.com/TurboCheetah/succubus.space-api#readme",
  "dependencies": {
    "@sentry/node": "7.120.4",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-core": "3.13.0",
    "apollo-server-express": "3.13.0",
    "bull": "4.16.5",
    "class-validator": "0.14.2",
    "compression": "1.8.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv-cra": "3.0.3",
    "express": "4.21.2",
    "graphql": "15.10.1",
    "helmet": "6.2.0",
    "hpp": "0.2.3",
    "ioredis": "5.7.0",
    "mongoose": "6.13.8",
    "morgan": "1.10.1",
    "nhentai": "3.0.0",
    "node-cron": "3.0.3",
    "phin": "3.7.1",
    "rate-limiter-flexible": "2.4.2",
    "redis-json": "6.0.3",
    "reflect-metadata": "0.2.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "tsyringe": "4.10.0",
    "type-graphql": "1.1.1",
    "typescript": "4.9.5",
    "winston": "3.17.0",
    "wireit": "0.14.12"
  },
  "devDependencies": {
    "@types/bull": "3.15.9",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/eslint": "8.56.12",
    "@types/express": "4.17.23",
    "@types/hpp": "0.2.6",
    "@types/ioredis": "4.28.10",
    "@types/jest": "30.0.0",
    "@types/morgan": "1.9.10",
    "@types/node": "16.18.126",
    "@types/node-cron": "3.0.11",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.5",
    "eslint-plugin-promise": "6.6.0",
    "husky": "8.0.3",
    "jest": "30.1.3",
    "lint-staged": "13.3.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "standard-version": "9.5.0",
    "supertest": "6.3.4",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2"
  },
  "lint-staged": {
    "src/*.ts": [
      "pnpm lint:fix",
      "pnpm test --bail --watchAll=false --findRelatedTests"
    ]
  },
  "packageManager": "pnpm@7.33.7"
}
