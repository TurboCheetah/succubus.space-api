{
  "name": "succubus.space",
  "version": "2.7.0",
  "description": "a fast API for fetching hentai data built with Redis and MongoDB",
  "author": "Turbo",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "start:docker": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc -p tsconfig.prod.json && tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint --fix && prettier --write src/",
    "docker": "docker-compose -f \"docker-compose.yml\" up -d --build",
    "docker:dev": "docker-compose -f \"docker-compose.dev.yml\" up -d --build",
    "test": "yarn docker:dev && jest",
    "patch": "standard-version --release-as patch",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/succubus.space-api.git"
  },
  "bugs": {
    "url": "https://github.com/TurboCheetah/succubus.space-api/issues"
  },
  "homepage": "https://github.com/TurboCheetah/succubus.space-api#readme",
  "dependencies": {
    "@sentry/node": "6.19.4",
    "apollo-server-cache-redis": "3.3.1",
    "apollo-server-express": "3.6.7",
    "bull": "4.8.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv-cra": "3.0.2",
    "express": "4.17.3",
    "graphql": "15.8.0",
    "helmet": "5.0.2",
    "hpp": "0.2.3",
    "ioredis": "5.0.3",
    "mongoose": "6.2.10",
    "morgan": "1.10.0",
    "nhentai": "3.0.0",
    "node-cron": "3.0.0",
    "phin": "3.6.1",
    "rate-limiter-flexible": "2.3.6",
    "redis-json": "6.0.3",
    "reflect-metadata": "0.1.13",
    "tsc-alias": "1.6.5",
    "tsconfig-paths": "3.14.1",
    "tsyringe": "4.6.0",
    "type-graphql": "1.1.1",
    "typescript": "4.6.3",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/bull": "3.15.8",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/eslint": "7.29.0",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/helmet": "4.0.0",
    "@types/hpp": "0.2.2",
    "@types/ioredis": "4.28.10",
    "@types/jest": "27.4.1",
    "@types/mongoose": "5.11.97",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.26",
    "@types/node-cron": "3.0.1",
    "@types/supertest": "2.0.12",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "standard-version": "9.3.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0"
  },
  "lint-staged": {
    "src/*.ts": [
      "yarn lint:fix",
      "yarn test --bail --watchAll=false --findRelatedTests"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
